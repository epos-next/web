{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "quotes": "off",
    "require-jsdoc": "warn",
    "object-curly-spacing": "warn",
    "indent": "off",
    "operator-linebreak": "off",
    "new-cap": "off"
  },
  "extends": [
    "airbnb"
  ]
}
