{
  "name": "epos-next",
  "version": "0.0.4",
  "private": true,
  "description": "epos-next",
  "author": "Yaroslav Zotov",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "develop": "NODE_ENV=development gatsby develop --https",
    "start": "NODE_ENV=production gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "storybook": "start-storybook -p 6006 -s ./static",
    "build-storybook": "build-storybook",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "test:e2e": "start-server-and-test develop http://localhost:8000 cy:open",
    "test:e2e:ci": "start-server-and-test develop http://localhost:8000 cy:run",
    "changelog": "auto-changelog --template changelog-template.hbs -p -u --commit-limit false",
    "changelog-debug": "auto-changelog --template changelog-template.hbs -p --template json --output changelog-data.json",
    "semantic-release": "semantic-release",
    "commit": "cz",
    "prepare": "husky install",
    "release": "standard-version"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.5.1",
    "axios": "^0.21.1",
    "axios-rate-limit": "^1.3.0",
    "babel-plugin-styled-components": "^1.12.0",
    "dotenv": "^10.0.0",
    "gatsby": "^3.6.2",
    "gatsby-plugin-manifest": "^3.7.0",
    "gatsby-plugin-offline": "^4.7.1",
    "gatsby-plugin-sharp": "^3.7.0",
    "gatsby-plugin-styled-components": "^4.6.0",
    "gatsby-plugin-use-query-params": "^1.0.1",
    "normalize.css": "^8.0.1",
    "react": "^17.0.1",
    "react-content-loader": "^6.0.3",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-redux": "^7.2.4",
    "react-ripples": "^2.2.1",
    "react-toastify": "^7.0.4",
    "redux": "^4.1.0",
    "styled-components": "^5.3.0",
    "universal-cookie": "^4.0.4",
    "use-query-params": "^1.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@reach/router": "^1.3.4",
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-docs": "^6.2.9",
    "@storybook/addon-essentials": "^6.2.9",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/react": "^6.2.9",
    "@testing-library/cypress": "^8.0.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/react-hooks": "^7.0.1",
    "@types/jest": "^27.0.0",
    "@types/node": "^15.12.1",
    "@types/react-helmet": "^6.1.1",
    "@types/react-redux": "^7.1.16",
    "@types/redux-mock-store": "^1.0.3",
    "@types/styled-components": "^5.1.9",
    "auto-changelog": "^2.3.0",
    "axe-core": "^4.3.2",
    "axios-mock-adapter": "^1.19.0",
    "babel-jest": "^27.0.6",
    "babel-loader": "^8.2.2",
    "babel-preset-gatsby": "^1.11.0",
    "commitizen": "^4.2.4",
    "cypress": "^8.2.0",
    "cypress-axe": "^0.12.2",
    "cypress-localstorage-commands": "^1.5.0",
    "cypress-plugin-snapshots": "^1.4.4",
    "cz-conventional-changelog": "3.3.0",
    "eslint-config-airbnb": "^18.2.1",
    "gatsby-plugin-sass": "^4.6.0",
    "gatsby-plugin-typescript": "^3.14.0",
    "husky": "^7.0.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.0.6",
    "jest-styled-components": "^7.0.5",
    "mockdate": "^3.0.5",
    "react-test-renderer": "^17.0.2",
    "redux-mock-store": "^1.5.4",
    "sass": "^1.34.1",
    "standard-version": "^9.3.1",
    "start-server-and-test": "^1.13.1",
    "ts-jest": "^27.0.4"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.json"
      }
    },
    "transform": {
      "\\.ts$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  },
  "globals": {
    "ts-jest": {
      "diagnostics": false
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/epos-next/web.git"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
